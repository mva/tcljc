             (NEW adder$capturing-inc) (DUP)
             (ALOAD_1)
             (ALOAD_0)
             (GETFIELD . "a" "I")
             (INVOKESPECIAL . "<init>" "(Lclojure/lang/IPersistentMap;I)V")
             (ARETURN)]]]
         (nmsp '[(ns pkg.ns0)
                 (def adder (fn* adder ^auto-return-type [^int a]
                                 (fn* capturing-inc ^int [^int b]
                                      (+ a b))))
                 (def five ((adder 2) 3))
                 (def seven ((adder 4) 3))]))))

